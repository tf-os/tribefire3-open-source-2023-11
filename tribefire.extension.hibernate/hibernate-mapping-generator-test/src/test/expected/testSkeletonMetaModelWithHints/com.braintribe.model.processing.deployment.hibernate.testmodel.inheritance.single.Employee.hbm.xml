<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
	"-//Hibernate/Hibernate Mapping DTD//EN" 
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true">
	<class name="com.braintribe.model.processing.deployment.hibernate.testmodel.inheritance.single.Employee" entity-name="com.braintribe.model.processing.deployment.hibernate.testmodel.inheritance.single.Employee" table="TEST_EMPLOYEE" abstract="false">
		<id name="id" column="ID" type="long">
			<generator class="native" />
		</id>
		<property name="employeeNumber" column="EMPLOYEENUMBER" type="long" />
		<property name="fullName" column="FULLNAME" type="string" />
		<property name="globalId" column="GLOBALID" type="string" />
		<property name="partition" column="PARTITION_" type="string" />
	</class>
</hibernate-mapping>
