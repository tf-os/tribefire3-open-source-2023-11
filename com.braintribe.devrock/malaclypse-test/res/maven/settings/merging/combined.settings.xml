<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<tf:settings xmlns:tf="http://maven.apache.org/SETTINGS/1.0.0">
    <tf:localRepository>recessive.d:/repo</tf:localRepository>
    <tf:interactiveMode>true</tf:interactiveMode>
    <tf:usePluginRegistry>true</tf:usePluginRegistry>
    <tf:offline>true</tf:offline>
    <tf:proxies>
        <tf:proxy>
            <tf:active>true</tf:active>
            <tf:protocol>http</tf:protocol>
            <tf:username>proxyuser</tf:username>
            <tf:password>somepassword</tf:password>
            <tf:port>8080</tf:port>
            <tf:host>proxy.somewhere.com</tf:host>
            <tf:nonProxyHosts>*.google.com|ibiblio.org</tf:nonProxyHosts>
            <tf:id>myProxy</tf:id>
        </tf:proxy>
        <tf:proxy>
            <tf:active>true</tf:active>
            <tf:protocol>http</tf:protocol>
            <tf:username>recessive.proxyuser</tf:username>
            <tf:password>recessive.somepassword</tf:password>
            <tf:port>8080</tf:port>
            <tf:host>recessive.proxy.somewhere.com</tf:host>
            <tf:nonProxyHosts>*.google.com|ibiblio.org</tf:nonProxyHosts>
            <tf:id>recessive.myproxy</tf:id>
        </tf:proxy>
    </tf:proxies>
    <tf:servers>
        <tf:server>
            <tf:username>builder</tf:username>
            <tf:password>operating2005</tf:password>
            <tf:filePermissions>664</tf:filePermissions>
            <tf:directoryPermissions>775</tf:directoryPermissions>
            <tf:id>central.mirror</tf:id>
        </tf:server>
        <tf:server>
            <tf:username>recessive.user</tf:username>
            <tf:password>recessive.pwd</tf:password>
            <tf:filePermissions>664</tf:filePermissions>
            <tf:directoryPermissions>775</tf:directoryPermissions>
            <tf:id>recessive.mirror</tf:id>
        </tf:server>
    </tf:servers>
    <tf:mirrors>
        <tf:mirror>
            <tf:mirrorOf>central</tf:mirrorOf>
            <tf:url>http://archiva.braintribe.com/repository/standalone/</tf:url>
            <tf:id>central.mirror</tf:id>
        </tf:mirror>
        <tf:mirror>
            <tf:mirrorOf>central</tf:mirrorOf>
            <tf:url>http://archiva.kwaqwagga.ch/repository/standalone/</tf:url>
            <tf:id>recessive.mirror</tf:id>
        </tf:mirror>
    </tf:mirrors>
    <tf:profiles>
        <tf:profile>
            <tf:activation>
                <activeByDefault>false</activeByDefault>
                <jdk>1.5</jdk>
                <os>
                    <name>Windows XP</name>
                    <family>Windows</family>
                    <arch>x86</arch>
                    <version>5.1.2600</version>
                </os>
                <property>
                    <name>mavenVersion</name>
                    <value>2.0.3</value>
                </property>
                <file>
                    <missing>${basedir}/file1.properties</missing>
                    <exists>${basedir}/file2.properties</exists>
                </file>
            </tf:activation>
            <properties>
      	<dominant_property_one>dominant_value</dominant_property_one>
      	<dominant_property_two>dominant_value</dominant_property_two>
      </properties>
            <tf:id>myProfile</tf:id>
        </tf:profile>
        <tf:profile>
            <tf:activation>
                <activeByDefault>false</activeByDefault>
                <jdk>1.5</jdk>
                <os>
                    <name>recessive.Windows XP</name>
                    <family>recessive.Windows</family>
                    <arch>recessive.x86</arch>
                    <version>recessive.5.1.2600</version>
                </os>
                <property>
                    <name>recessive.mavenVersion</name>
                    <value>recessive.2.0.3</value>
                </property>
                <file>
                    <missing>${basedir}/file1.recessive.properties</missing>
                    <exists>${basedir}/file2.recessive.properties</exists>
                </file>
            </tf:activation>
            <properties>
      	<recessive_property_one>recessive_value</recessive_property_one>
      	<recessive_property_two>recessive_value</recessive_property_two>
      </properties>
            <tf:id>recessive.myProfile</tf:id>
        </tf:profile>
    </tf:profiles>
    <tf:pluginGroups>
        <tf:pluginGroup>org.mortbay.jetty</tf:pluginGroup>
        <tf:pluginGroup>recessive.org.mortbay.jetty</tf:pluginGroup>
    </tf:pluginGroups>
</tf:settings>
